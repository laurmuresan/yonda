<!DOCTYPE html>
<html>
<head>
    <!-- CSS (load bootstrap) -->
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="myApp/css/site.css" />
    <style>
        .navbar {
            border-radius: 0;
        }
    </style>

    <!-- JS (load angular, ui-router, and our custom js file) -->
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.3.5/angular.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.8/angular-ui-router.min.js"></script>
    <script src="/myApp/app.js"></script>
    <script type="text/javascript" src="/myApp/services/AuthenticationService.js"></script>
    <script type="text/javascript" src="/myApp/pages/login/login.controller.js"></script>
    <script type="text/javascript" src="/myApp/services/AlleventsService.js"></script>
    <script type="text/javascript" src="/myApp/pages/allevents/allevents.controller.js"></script>
</head>
<body ng-app="myApp">

    <!-- NAVIGATION -->
    <nav class="navbar navbar-inverse" role="navigation">
        <div class="navbar-header">
            <a class="navbar-brand" ui-sref="#">ngEvents</a>
        </div>
        <ul class="nav navbar-nav">
            <li><a ui-sref="allevents">All Events</a></li>
            <li><a ui-sref="createevent">Create Event</a></li>
            <li>
                <form _ngcontent-hip-1="" class="navbar-form navbar-right ng-untouched ng-pristine ng-valid" id="searchForm">
                        <div _ngcontent-hip-1="" class="form-group">
                            <input _ngcontent-hip-1="" class="form-control ng-untouched ng-pristine ng-valid" name="searchTerm" placeholder="Search Sessions" type="text" ng-reflect-name="searchTerm">
                        </div>
                        <button _ngcontent-hip-1="" class="btn btn-default" modal-trigger="searchResults" ng-reflect-modal-id="searchResults">
                            Search
                        </button>
                    </form>
                </li>
            <li><a ui-sref="login">Login</a></li>
        </ul>
    </nav>

    <div class="container">

  
        <div ui-view></div>

    </div>
</body>
</html>